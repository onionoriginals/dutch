{
  "name": "@originals/dutch",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc -p tsconfig.json && esbuild src/index.ts --bundle --format=cjs --platform=node --outfile=dist/index.cjs",
    "dev": "tsc -p tsconfig.json --watch"
  },
  "dependencies": {
    "bip32": "^5.0.0-rc.0",
    "bip39": "^3.1.0",
    "bitcoinjs-lib": "^6.1.7",
    "crypto-js": "^4.2.0",
    "tiny-secp256k1": "^2.2.4"
  }
}
