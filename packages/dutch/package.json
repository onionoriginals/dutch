{
  "name": "@originals/dutch",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc -p tsconfig.json && esbuild src/index.ts --bundle --format=cjs --platform=node --outfile=dist/index.cjs",
    "dev": "tsc -p tsconfig.json --watch"
  }
}
